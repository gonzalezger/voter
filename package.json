{
  "name": "voter",
  "version": "1.0.0",
  "description": "Real-time voting system",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server:start-dev\" \"npm run client:start\"",
    "postinstal": "npm install --prefix server",
    "server:start-dev": "cd server && npm run dev",
    "server:start": "cd server && node server.js",
    "client:start": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gonzalezger/voter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gonzalezger/voter/issues"
  },
  "homepage": "https://github.com/gonzalezger/voter#readme",
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
